{
  "name": "type-challenges-judge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "cloud-functions",
    "judge-worker"
  ],
  "scripts": {
    "dev": "NO_D1_WARNING=true remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "build": "remix build",
    "emulators": "firebase emulators:start --only auth",
    "db:path": "echo '.wrangler/state/v3/d1/2adb5da8-cd23-45a7-b2fb-2392319e9520/db.sqlite'",
    "db:seed": "node database/seed-runner.js",
    "db:console": "sqlite3 $(yarn run --silent db:path)",
    "db:dump-schema": "sqlite3 $(yarn run --silent db:path) .schema > database/schema.sql",
    "generate:query": "sqlc generate",
    "migrate": "wrangler d1 migrations apply type-challenges-judge --local",
    "migrate:reset": "rm $(yarn run --silent db:path) && yarn run migrate",
    "migrate:production": "wrangler d1 migrations apply type-challenges-judge",
    "lint": "yarn run lint:prettier && yarn run lint:eslint && yarn run lint:tsc",
    "lint:prettier": "prettier --check --ignore-path .gitignore '**/*.{js,ts,tsx}' '!server/query'",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:tsc": "tsc --noEmit",
    "copy-typescript-lib": "node scripts/copy-typescript-lib.js"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.0.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@remix-run/cloudflare": "^1.19.1",
    "@remix-run/cloudflare-pages": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "chart.js": "^3.8.0",
    "firebase": "^9.8.1",
    "firebase-auth-cloudflare-workers": "^1.0.1",
    "framer-motion": "^6",
    "isbot": "^3.6.13",
    "lz-string": "^1.5.0",
    "react": "^18.0.0",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^8.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "tiny-invariant": "^1.3.1",
    "typescript": "4.6.3",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230724.0",
    "@firebase/rules-unit-testing": "^2.0.2",
    "@remix-run/dev": "^1.19.1",
    "@types/eslint": "^8.4.2",
    "@types/prettier": "^2.6.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-syntax-highlighter": "^15.5.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.15.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "firebase-tools": "^10.9.2",
    "prettier": "^2.6.2",
    "vite": "^2.9.9",
    "vitest": "^0.13.0",
    "wrangler": "^3.4.0"
  },
  "engines": {
    "yarn": "1"
  }
}
